#include <stdio.h>

int main() {
    char ch;
    int whitespaces = 0, newlines = 0;

    printf("Enter text (end input with #):\n");

    while ((ch = getchar()) != '#' && ch != EOF) {
        if (ch == ' ' || ch == '\t') {
            whitespaces++;
        } else if (ch == '\n') {
            newlines++;
        }
    }

    printf("\nNumber of whitespaces: %d\n", whitespaces);
    printf("Number of newlines: %d\n", newlines);

    return 0;
}
